{
  "name": "jakets",
  "version": "7.1.1",
  "description": "Jake with TypeScript.",
  "main": "Jakefile.js",
  "scripts": {
    "build:compile": "tsc -p ./tsconfig.json",
    "build:test": "jake jts_test",
    "build": "npm run build:compile && npm run build:test",
    "postinstall": "node ./lib/Postinstall.js",
    "jakets:build": "jake -t --jakefile Jakefile.js",
    "jakets:setup": "jake -t --jakefile Jakefile.js jts:setup",
    "jakets": "npm run jakets:setup && npm run jakets:build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reshadi/jakets.git"
  },
  "author": "Mehrdad Reshadi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reshadi/jakets/issues"
  },
  "homepage": "https://github.com/reshadi/jakets",
  "dependencies": {
    "@types/jake": "^0.0.30",
    "@types/semver": "^5.5.0",
    "jake": "^8.0.16",
    "semver": "^5.5.0",
    "tslib": "^1.9.3",
    "typescript": ">=2.7.2"
  }
}
